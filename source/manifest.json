{

    "manifest_version": 2,
    "name": "turbo",
    "version": "0.0.2",
    "applications": {
      "gecko": {
        "strict_min_version": "52.0a1"
      }
    },
    "description": "superfast navigation via fulltext search",
  
    "permissions": [
      "*://*/navmap.json",
      "webNavigation",
      "tabs",
      "storage"
    ],

    "omnibox": { "keyword" : ">" },

    "background": {
      "scripts": [
        "const.js",
        "search/source/search.js",
        "remotes.js",
        "background.js"
      ]
    },

    "icons": {
      "32": "icons/turbo-30.png",
      "8": "icons/turbo-30.png",
      "16": "icons/turbo-30.png",
      "48": "icons/turbo-30.png",
      "96": "icons/turbo-30.png"
    },

    "browser_action": {
      "default_icon": "icons/turbo-30.png"
    }

  }
  